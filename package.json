    "@angular/language-service": "^12.1.0",
    "@ngrx/schematics": "12.2.0",
    "@ngrx/store-devtools": "^12.2.0",
    "@nrwl/cypress": "12.5.0",
    "@nrwl/eslint-plugin-nx": "12.5.0",
    "@nrwl/jest": "12.5.0",
    "@nrwl/linter": "12.5.0",
    "@nrwl/workspace": "12.5.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "26.0.8",
