    "@ngrx/store": "^12.2.0",
    "@nrwl/angular": "12.5.0",
    "apollo-angular": "^2.0.3",
    "core-js": "^2.5.4",
    "graphql": "^15.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.0",
